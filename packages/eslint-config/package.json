{
  "name": "@ehubbell/eslint-config",
  "version": "0.0.1",
  "description": "A collection of configs used for various projects.",
  "author": "Eric Hubbell<eric@erichubbell.com>",
  "license": "ISC",
  "main": "./src/eslint-config-web.js",
  "files": [
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehubbell/configs.git"
  },
  "keywords": [
    "playbooks",
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier src",
    "format:fix": "prettier --write src",
    "clean": "npm run lint:fix && npm run format:fix",
    "commit": "scripts/commit.sh",
    "deploy": "scripts/deploy.sh",
    "yalc": "npx yalc push"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.5",
    "typescript": "^5"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^15.3.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.5",
    "typescript": "^5"
  }
}
